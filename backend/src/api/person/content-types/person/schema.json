{
  "kind": "collectionType",
  "collectionName": "people",
  "info": {
    "singularName": "person",
    "pluralName": "people",
    "displayName": "Person"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string"
    },
    "nacionalidad": {
      "type": "string"
    },
    "birth_date": {
      "type": "date"
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "phone_number": {
      "type": "biginteger"
    },
    "cedula_rif": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "es_juridico": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "person_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::person-type.person-type",
      "inversedBy": "types"
    }
  }
}
